

<!DOCTYPE html>
<html lang="zxx">

<head>
    <meta charset="UTF-8">
    <meta name="description" content="Fashi Template">
    <meta name="keywords" content="Fashi, unica, creative, html">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Fashi | Template</title>

    <!-- Google Font -->
    <link href="https://fonts.googleapis.com/css?family=Muli:300,400,500,600,700,800,900&display=swap" rel="stylesheet">

    <!-- Css Styles -->
    <link rel="stylesheet" href="~/AdminLTE-3.0.4/fashi/css/bootstrap.min.css" type="text/css">
    <link rel="stylesheet" href="~/AdminLTE-3.0.4/fashi/css/font-awesome.min.css" type="text/css">
    <link rel="stylesheet" href="~/AdminLTE-3.0.4/fashi/css/themify-icons.css" type="text/css">
    <link rel="stylesheet" href="~/AdminLTE-3.0.4/fashi/css/elegant-icons.css" type="text/css">
    <link rel="stylesheet" href="~/AdminLTE-3.0.4/fashi/css/owl.carousel.min.css" type="text/css">
    <link rel="stylesheet" href="~/AdminLTE-3.0.4/fashi/css/nice-select.css" type="text/css">
    <link rel="stylesheet" href="~/AdminLTE-3.0.4/fashi/css/jquery-ui.min.css" type="text/css">
    <link rel="stylesheet" href="~/AdminLTE-3.0.4/fashi/css/slicknav.min.css" type="text/css">
    <link rel="stylesheet" href="~/AdminLTE-3.0.4/fashi/css/style.css" type="text/css">
</head>

<body>
    <!-- Page Preloder -->
    <div id="preloder">
        <div class="loader"></div>
    </div>

    <!-- Header Section Begin -->
    <header class="header-section">
        <div class="header-top">
            <div class="container">
                <div class="ht-left">
                    <div class="mail-service">
                        <i class=" fa fa-envelope"></i>
                        mvcproje@gmail.com
                    </div>
                    <div class="phone-service">
                        <i class=" fa fa-phone"></i>
                        +90 0530 123 21 22
                    </div>
                </div>
                <div class="ht-right">
                    @if (!User.IsInRole("kullanıcı") && (!User.IsInRole("admin")))
                    {
                        <a href="/Login/OrderLogin/" class="login-panel"><i class="fa fa-user"></i>Giriş Yap</a>

                        <a href="/Register/OrderRegister/" style="padding-right:10px" class="login-panel"><i class="fa fa-user"></i>Kayıt Ol</a>
                    }
                    @if (User.IsInRole("kullanıcı"))
                    {
                        <a href="#" class="login-panel"><i class="fa fa-user"></i>@HttpContext.Current.User.Identity.Name</a>
                        <a href="/Login/LogOut/" style="padding-right:10px" class="login-panel"><i class="fa fa-user"></i>Çıkış Yap</a>
                    }
                    <div class="top-social">
                        <a href="#"><i class="ti-facebook"></i></a>
                        <a href="#"><i class="ti-twitter-alt"></i></a>
                        <a href="#"><i class="ti-linkedin"></i></a>
                        <a href="#"><i class="ti-pinterest"></i></a>
                    </div>
                </div>
            </div>
        </div>
        <div class="container">
            <div class="inner-header">
                <div class="row">
                    <div class="col-lg-2 col-md-2">
                        <div class="logo">
                            <a href="/Home/HomePage/">
                                <img src="/AdminLTE-3.0.4/fashi/img/logo.png" alt="">
                            </a>
                        </div>
                    </div>
                    <div class="col-lg-7 col-md-7">
                        <div class="advanced-search">
                            <button type="button" class="category-btn" style="padding-top:1px">Tüm Kategoriler</button>
                            <div class="input-group">
                                @using (Html.BeginForm("Index", "Home", FormMethod.Post))
                                {
                                    <p>
                                        @Html.TextBox("p")
                                    </p>
                                    <button type="button"><i class="ti-search"></i></button>
                                }
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-3 text-right col-md-3">
                        <ul class="nav-right">
                            <li class="heart-icon">
                                <a href="#">
                                    <i class="icon_heart_alt"></i>
                                    <span>1</span>
                                </a>
                            </li>
                            <li class="cart-icon">
                                <a href="#">
                                    <i class="icon_bag_alt"></i>
                                    <span>@{Html.RenderAction("TotalCount", "ShoppingCart");}</span>
                                </a>
                                <div class="cart-hover">
                                    <div class="select-items">
                                        <table>
                                            <tbody>
                                                <tr>
                                                    <td class="si-pic"><img src="/AdminLTE-3.0.4/fashi/img/select-product-1.jpg" alt=""></td>
                                                    <td class="si-text">
                                                        <div class="product-selected">
                                                            <p>$60.00 x 1</p>
                                                            <h6>Kabino Bedside Table</h6>
                                                        </div>
                                                    </td>
                                                    <td class="si-close">
                                                        <i class="ti-close"></i>
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <td class="si-pic"><img src="/AdminLTE-3.0.4/fashi/img/select-product-2.jpg" alt=""></td>
                                                    <td class="si-text">
                                                        <div class="product-selected">
                                                            <p>$60.00 x 1</p>
                                                            <h6>Kabino Bedside Table</h6>
                                                        </div>
                                                    </td>
                                                    <td class="si-close">
                                                        <i class="ti-close"></i>
                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                    <div class="select-total">
                                        <span>Toplam:</span>
                                        <h5>$120.00</h5>
                                    </div>
                                    <div class="select-button">
                                        <a href="#" class="primary-btn view-card">Kartları Görüntüle</a>
                                        <a href="#" class="primary-btn checkout-btn">Ödeme</a>
                                    </div>
                                </div>
                            </li>
                            <li class="cart-price">$150.00</li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
        <div class="nav-item">
            <div class="container">
                <div class="nav-depart">
                    <div class="depart-btn">
                        <i class="ti-menu"></i>
                        <span>Kategoriler</span>
                        <ul class="depart-hover">
                            @{
                                    var db = new ProductTrackingSystem.DataAccess.ProductTrackingDbContext();
                                    var model = db.Categories.ToList();
                                    foreach (var item in model)
                                    {
                                    <li class="active"><a href="/Home/ProductsByCategory/@item.CategoryId">@item.CategoryName</a></li>
                                    }
                            }
                        </ul>
                    </div>
                </div>
                <nav class="nav-menu mobile-menu">
                    <ul>
                        <li class="active"><a href="/Home/HomePage/">Ana Sayfa</a></li>
                        <li><a href="/Home/Index/">Ürünler</a></li>
                        <li><a href="/AdminLTE-3.0.4/fashi/blog.html">Blog</a></li>
                        <li><a href="/HomeContact/AddContact/">İletişim</a></li>
                        <li>
                            <a href="#">Sayfalar</a>
                            <ul class="dropdown">
                                <li><a href="/AdminLTE-3.0.4/fashi/blog-details.html">Blog Detay</a></li>
                                <li><a href="/ShoppingCart/Index/">Alışveriş Sepeti</a></li>
                                <li><a href="/AdminLTE-3.0.4/fashi/check-out.html">Ödeme</a></li>
                                <li><a href="/AdminLTE-3.0.4/fashi/faq.html">SSS</a></li>
                                <li><a href="/Register/OrderRegister/">Kayıt Ol</a></li>
                                <li><a href="/Login/OrderLogin/">Giriş Yap</a></li>
                            </ul>
                        </li>
                    </ul>
                </nav>
                <div id="mobile-menu-wrap"></div>
            </div>
        </div>
    </header>
    <!-- Header End -->
    <div class="container">
        @RenderBody()
    </div>
    <!-- Partner Logo Section Begin -->
    <div class="partner-logo">
        <div class="container">
            <div class="logo-carousel owl-carousel">
                <div class="logo-item">
                    <div class="tablecell-inner">
                        <img src="/AdminLTE-3.0.4/fashi/img/logo-carousel/logo-1.png" alt="">
                    </div>
                </div>
                <div class="logo-item">
                    <div class="tablecell-inner">
                        <img src="/AdminLTE-3.0.4/fashi/img/logo-carousel/logo-2.png" alt="">
                    </div>
                </div>
                <div class="logo-item">
                    <div class="tablecell-inner">
                        <img src="/AdminLTE-3.0.4/fashi/img/logo-carousel/logo-3.png" alt="">
                    </div>
                </div>
                <div class="logo-item">
                    <div class="tablecell-inner">
                        <img src="/AdminLTE-3.0.4/fashi/img/logo-carousel/logo-4.png" alt="">
                    </div>
                </div>
                <div class="logo-item">
                    <div class="tablecell-inner">
                        <img src="/AdminLTE-3.0.4/fashi/img/logo-carousel/logo-5.png" alt="">
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Partner Logo Section End -->
    <!-- Footer Section Begin -->
    <footer class="footer-section">
        <div class="container">
            <div class="row">
                <div class="col-lg-3">
                    <div class="footer-left">
                        <div class="footer-logo">
                            <a href="#"><img src="/AdminLTE-3.0.4/fashi/img/footer-logo.png" alt=""></a>
                        </div>
                        <ul>
                            <li>Adres: YTÜ Teknopark</li>
                            <li>Telefon: +90 0530 123 21 22</li>
                            <li>Email: mvcproje@gmail.com</li>
                        </ul>
                        <div class="footer-social">
                            <a href="#"><i class="fa fa-facebook"></i></a>
                            <a href="#"><i class="fa fa-instagram"></i></a>
                            <a href="#"><i class="fa fa-twitter"></i></a>
                            <a href="#"><i class="fa fa-pinterest"></i></a>
                        </div>
                    </div>
                </div>
                <div class="col-lg-2 offset-lg-1">
                    <div class="footer-widget">
                        <h5>Bilgi</h5>
                        <ul>
                            <li><a href="#">Hakkında</a></li>
                            <li><a href="#">Ödeme</a></li>
                            <li><a href="#">İletişim</a></li>
                            <li><a href="#">Serivius</a></li>
                        </ul>
                    </div>
                </div>
                <div class="col-lg-2">
                    <div class="footer-widget">
                        <h5>Hesabım</h5>
                        <ul>
                            <li><a href="#">Hesabım</a></li>
                            <li><a href="#">İletişim</a></li>
                            <li><a href="#">Alışveriş Sepeti</a></li>
                            <li><a href="#">Ürünler</a></li>
                        </ul>
                    </div>
                </div>
                <div class="col-lg-4">
                    <div class="newslatter-item">
                        <h5>Bültenimize şimdi katılın.</h5>
                        <p>Mağazamız ve özel tekliflerimiz hakkında e-posta güncellemesi alın.</p>
                        <form action="#" class="subscribe-form">
                            <input type="text" placeholder="Mail">
                            <button type="button">Abone Ol</button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
        <div class="copyright-reserved">
            <div class="container">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="copyright-text">
                            <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                            Copyright &copy;
                            <script>document.write(new Date().getFullYear());</script> All rights reserved | This template is made with <i class="fa fa-heart-o" aria-hidden="true"></i> by <a href="https://colorlib.com" target="_blank">Colorlib</a>
                            <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                        </div>
                        <div class="payment-pic">
                            <img src="/AdminLTE-3.0.4/fashi/img/payment-method.png" alt="">
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </footer>
    <!-- Footer Section End -->
    <!-- Js Plugins -->
    <script src="~/AdminLTE-3.0.4/fashi/js/jquery-3.3.1.min.js"></script>
    <script src="~/AdminLTE-3.0.4/fashi/js/bootstrap.min.js"></script>
    <script src="~/AdminLTE-3.0.4/fashi/js/jquery-ui.min.js"></script>
    <script src="~/AdminLTE-3.0.4/fashi/js/jquery.countdown.min.js"></script>
    <script src="~/AdminLTE-3.0.4/fashi/js/jquery.nice-select.min.js"></script>
    <script src="~/AdminLTE-3.0.4/fashi/js/jquery.zoom.min.js"></script>
    <script src="~/AdminLTE-3.0.4/fashi/js/jquery.dd.min.js"></script>
    <script src="~/AdminLTE-3.0.4/fashi/js/jquery.slicknav.js"></script>
    <script src="~/AdminLTE-3.0.4/fashi/js/owl.carousel.min.js"></script>
    <script src="~/AdminLTE-3.0.4/fashi/js/main.js"></script>
</body>

</html>



